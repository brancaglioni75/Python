from datetime import date
atual = date.today().year
nasc = int(input('Ano de Nascimento: '))
idade = atual - nasc
print('O atleta tem {} anos.'.format(idade))
if idade <= 9:
    print('Classificação {}'.format('MIRIM'))
elif idade <= 14:
    print('Classificação {}'.format('INFANTIL'))
elif idade <= 19:
    print('Classificação {}'.format('JÚNIOR'))
elif idade <= 25:
    print('Classificação {}'.format('SENIOR'))
else:
    print('Classificação {}'.format('MASTER'))
